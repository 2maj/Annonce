{% block body %}
    <table border="1">
        <tr>
            <th>Id</th><th>Nom</th><th>Prix</th>
        </tr>
        {% for produit in produits %}
            <tr>
                <td>{{ produit.id }}</td><td>{{ produit.nom }}</td><td>{{ produit.prix }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}